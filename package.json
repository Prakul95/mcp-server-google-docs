{
  "name": "mcp-server-google-docs",
  "version": "1.0.0",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "google-calendar-mcp": "build/index.js"
  },
  "files": [
    "build/",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "claude",
    "google-calendar",
    "calendar",
    "ai",
    "llm",
    "integration"
  ],
  "scripts": {
    "start": "tsx src/server.ts",
    "build": "npm run lint:fix && node scripts/build.js",
    "auth": "node build/auth-server.js",
    "dev": "node scripts/dev.js",
    "test": "vitest run src/tests/unit",
    "test:watch": "vitest src/tests/unit",
    "test:integration": "vitest run src/tests/integration",
    "test:all": "vitest run src/tests",
    "test:coverage": "vitest run src/tests/unit --coverage",
    "start:http": "node build/index.js --transport http --port 3000",
    "start:http:public": "node build/index.js --transport http --port 3000 --host 0.0.0.0",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "format": "prettier --write ."
  },
  "description": "",
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "esbuild": "^0.25.0",
    "google-auth-library": "^10.2.1",
    "googleapis": "^155.0.1",
    "jiti": "^2.5.1",
    "open": "^10.2.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "openai": "^4.104.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.1.1"
  }
}
